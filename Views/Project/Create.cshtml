@model danielmoorhouse.Data.Projects

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>

<h4>Projects</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-controller="Project" asp-action="Create" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Title" class="control-label"></label>
                <input asp-for="Title" class="form-control" />
                <span asp-validation-for="Title" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ProjDesc" class="control-label"></label>
                <input asp-for="ProjDesc" class="form-control" />
                <span asp-validation-for="ProjDesc" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="GitRepo" class="control-label"></label>
                <input asp-for="GitRepo" class="form-control" />
                <span asp-validation-for="GitRepo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PImage1" class="control-label"></label>
                <input asp-for="PImage1" class="form-control" />
                <span asp-validation-for="PImage1" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PImage2" class="control-label"></label>
                <input asp-for="PImage2" class="form-control" />
                <span asp-validation-for="PImage2" class="text-danger"></span>
            </div>
                 <div class="form-group">
                <label asp-for="PImage3" class="control-label"></label>
                <input asp-for="PImage3" class="form-control" />
                <span asp-validation-for="PImage3" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PImage4" class="control-label"></label>
                <input asp-for="PImage4" class="form-control" />
                <span asp-validation-for="PImage4" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

</body>
</html>
